{
    "ver": "0.0.1",
    "authors": [
        "qzhang"
    ],
    "contact": "help@kbase.us",
    "categories": ["active", "assembly"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "jf_size",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["2000000000"],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 2000000000
            }
        },
        {
            "id": "reads_id",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseAssembly.PairedEndLibrary","KBaseFile.PairedEndLibrary"]
            }
        },
        {
            "id": "reads_prefix",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "pe" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "text"
            }
        },
        {
            "id": "reads_mean",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "180" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
                "min_int" : 0
            }
        },
        {
            "id": "reads_stdv",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "20.0" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
                "min_float" : 20.0
            }
        },
        {
            "id": "pacbio_reads",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseAssembly.PairedEndLibrary","KBaseFile.PairedEndLibrary"]
            }
        },
        {
            "id": "other_frg_file",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseAssembly.PairedEndLibrary","KBaseFile.PairedEndLibrary"]
            }
        },
        {
            "id": "output_contigset_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["masurca.contigs"],
            "field_type": "text",
            "text_options": {
                "valid_ws_types" : [ "KBaseGenomeAnnotations.Assembly" ],
                "is_output_name":true
            }
        },
        {
            "id": "graph_kmer_size",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "auto", "int" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "text",
                "min_int" : 1 
            }
        },
        {
            "id": "use_linking_mates",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "checkbox",
                "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "limit_jump_coverage",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "60" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
                "min_int" : 60,
                "max_int" : 300
            }
        },
        {
            "id": "cgwErrorRate",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.25" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float"
            }
        },
        {
            "id": "close_gaps",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "soap_assembly",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "do_homopolymer_trim",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "kmer_count_threshold",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : ["int"]
            }
        }
    ],
    "parameter-groups": [
      {
        "id": "reads_libraries",
        "parameters": ["reads_id", "reads_prefix", "reads_mean", "reads_stdv"],
        "optional": false,
        "allow_multiple": false,
        "with_border": true
      }
    ],
    "parameter-groups": [
      {
        "id": "jump_libraries",
        "parameters": ["reads_id", "reads_prefix", "reads_mean", "reads_stdv"],
        "optional": true,
        "allow_multiple": false,
        "with_border": true
      }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"MaSuRCA",
            "method": "run_masurca",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "reads_id",
                    "target_property": "reads_id",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "jf_size",
                    "target_property": "jf_size"
                },
                {
                    "input_parameter": "reads_libraries",
                    "target_property": "reads_libraries"
                },
		{
                    "input_parameter": "jump_libraries",
                    "target_property": "jump_libraries"
                },
                {
                    "input_parameter": "pacbio_reads",
                    "target_property": "pacbio_reads",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "output_contigset_name",
                    "target_property": "output_contigset_name"
                },
                {
                    "input_parameter": "other_frg_file",
                    "target_property": "other_frg_file"
                },
                {
                    "input_parameter": "graph_kmer_size",
                    "target_property": "graph_kmer_size"
                },
                {
                    "input_parameter": "use_linking_mates",
                    "target_property": "use_linking_mates"
                },
                {
                    "input_parameter": "limit_jump_coverage",
                    "target_property": "limit_jump_coverage"
                },
                {
                    "input_parameter": "cgwErrorRate",
                    "target_property": "cgwErrorRate"
                },
                {
                    "input_parameter": "kmer_count_threshold",
                    "target_property": "kmer_count_threshold"
                },
                {
                    "input_parameter": "close_gaps",
                    "target_property": "close_gaps"
                },
                {
                    "input_parameter": "soap_assembly",
                    "target_property": "soap_assembly"
                },
                {
                    "input_parameter": "do_homopolymer_trim",
                    "target_property": "do_homopolymer_trim"
                },
                {
                    "input_parameter": "reads_libraries",
                    "target_property": "reads_libraries"
	            },
                {
                    "input_parameter": "jump_libraries",
                    "target_property": "jump_libraries"
	            },
                {
                    "constant_value": 1,
                    "target_property": "create_report"
                },
                {            
                    "constant_value"  : 2,
                    "target_property" : "num_threads"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                },
                {
                    "input_parameter": "read_libraries",
                    "target_property": "input_read_libraries"
                },
                {
                    "input_parameter": "output_contigset_name",
                    "target_property": "input_output_contigset_name"
                },
                {
                    "input_parameter": "jf_size",
                    "target_property": "jf_size"
                },
                {
                    "service_method_output_path": [0],
                    "target_property": "masurca_result"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
